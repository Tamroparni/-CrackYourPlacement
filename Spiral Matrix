class Solution {
    public List<Integer> spiralOrder(int[][] matrix) {
        ArrayList<Integer> list = new ArrayList<>();
        int top =0;
        int left = 0;
        int right = matrix.length-1;
        int bottom = matrix[0].lentgh-1;
        while(top <= bottom && left <= right){
            for(int i= left; i < right; i++){
                
            }
        }
        return list;
    }
}
